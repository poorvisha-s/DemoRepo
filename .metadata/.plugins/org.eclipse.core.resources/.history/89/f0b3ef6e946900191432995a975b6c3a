package weektwo;

public class Sample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int a = 10;
if(a>10) {
	System.out.println("The value of a is greater than 10");
} else if(a<10) {
	System.out.println("The value of a is less than 10");
} else { 
	System.out.println("The value of a is equal to 10");
	}
int i = 0;
while (a>5)
{
	System.out.println("Testing while loop" +i+1);
	a--;
	i++;
}
for (i = 0; i<10; i++)
{
	System.out.println("Testing for loop");
}

	}

}
